journal = {BMC genomics},
keywords = {Alternative Splicing,Alternative Splicing: genetics,Exons,Exons: genetics,High-Throughput Nucleotide Sequencing,Models, Genetic,Molecular Sequence Annotation,Molecular Sequence Annotation: methods,RNA, Long Noncoding,RNA, Long Noncoding: genetics,Toxoplasma,Toxoplasma: genetics,Transcriptome,Transcriptome: genetics},
